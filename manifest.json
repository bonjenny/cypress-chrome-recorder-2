{
  "manifest_version": 3,
  "name": "Cypress Stringify Extension",
  "version": "1.0.0",
  "description": "Extension to modify Cypress selectors for E2E testing",
  "background": {
    "service_worker": "dist/CypressStringifyExtension.js",
    "type": "module"
  },
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"]
}
